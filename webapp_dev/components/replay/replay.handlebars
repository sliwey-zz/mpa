<div id="{{ id }}" class="replay-wrap">
  <div class="replay-head">
    <span class="replay-title">轨迹回放控制</span>
    <span class="replay-no">浙BT{{ number }}</span>
  </div>
  <span class="replay-close" title="关闭">&times;</span>
  <div class="replay-body">
    <div class="replay-chart"></div>
  </div>
  <div class="replay-foot">
    <div class="replay-btn play"></div>
    <div class="replay-progress">
      <div class="replay-progress-bar" style="width:0%">
        <span class="replay-progress-ctrl"></span>
      </div>
    </div>
  </div>
</div>